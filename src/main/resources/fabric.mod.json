{
  "schemaVersion": 1,
  "id": "saturns_origins",
  "version": "${version}",
  "name": "Saturns Origins",
  "description": "Adds a new origin, the Chorus Fruit Born!",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "accessWidener" : "saturns_origins.accesswidener",
  "icon": "assets/saturns_origins/icon.png",
  "environment": "*",
  "entrypoints": {
    "cardinal-components": [
      "rings_of_saturn.github.io.saturns_origins.components.ModComponents"
    ],
    "fabric-datagen": [
      "rings_of_saturn.github.io.saturns_origins.client.SaturnsOriginsDataGenerator"
    ],
    "client": [
      "rings_of_saturn.github.io.saturns_origins.client.SaturnsOriginsClient"
    ],
    "main": [
      "rings_of_saturn.github.io.saturns_origins.SaturnsOrigins"
    ]
  },
  "mixins": [
    "saturns_origins.mixins.json",
    {
      "config": "saturns_origins.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "cardinal-components": [
      "saturns_origins:portal_position",
      "saturns_origins:portal_dim",
      "saturns_origins:backstab_cooldown",
      "saturns_origins:portal_cooldown",
      "saturns_origins:invis_cooldown",
      "saturns_origins:bloodlust_cooldown",
      "saturns_origins:has_tp",
      "saturns_origins:swarm_attack"
    ]
  }
}
